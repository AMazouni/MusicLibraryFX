<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="songRow" alignment="BASELINE_LEFT" fillHeight="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="songButton" mnemonicParsing="false" onAction="#SongButtonClick" text="Button" />
      <Label fx:id="songLabel" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onMouseClicked="#OnClick" text="Label" HBox.hgrow="ALWAYS">
         <padding>
            <Insets left="50.0" right="50.0" />
         </padding>
      </Label>
      <Label fx:id="songArtist" text="Label" HBox.hgrow="ALWAYS">
         <padding>
            <Insets left="50.0" right="50.0" />
         </padding>
      </Label>
      <Label fx:id="songAlbum" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Label" HBox.hgrow="SOMETIMES">
         <padding>
            <Insets left="50.0" right="50.0" />
         </padding>
      </Label>
      <Label fx:id="songGenre" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Label" HBox.hgrow="ALWAYS">
         <padding>
            <Insets left="50.0" right="50.0" />
         </padding>
      </Label>
   </children>
   <padding>
      <Insets bottom="20.0" left="30.0" />
   </padding>
</HBox>
