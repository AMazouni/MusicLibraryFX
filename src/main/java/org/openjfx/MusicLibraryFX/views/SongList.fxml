<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="stackpane" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.MusicLibraryFX.controllers.SongListController">
   <children>
      <ScrollPane fitToWidth="true" hbarPolicy="NEVER">
         <content>
            <TableView fx:id="tableView" editable="true" fixedCellSize="10.0" tableMenuButtonVisible="true">
               <columns>
                  <TableColumn fx:id="Libelle" prefWidth="133.0" text="Song" />
                  <TableColumn fx:id="artist" prefWidth="114.0" text="Artist" />
                  <TableColumn fx:id="album" prefWidth="125.0" text="Album" />
                  <TableColumn fx:id="ReleaseDate" prefWidth="188.0" text="Release Date" />
                  <TableColumn prefWidth="39.0" />
               </columns>
            </TableView>
         </content>
      </ScrollPane>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Button fx:id="Reload" layoutX="525.0" layoutY="348.0" mnemonicParsing="false" onMouseClicked="#ReloadList" prefHeight="38.0" prefWidth="54.0" text="Reload" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
